    "@types/react-fontawesome": "^1.6.4",
    "@types/wavesurfer.js": "^2.0.2",
    "axios": "^0.19.0",
    "babel-eslint": "10.0.1",
    "chokidar": "^3.0.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "dts-gen": "^0.5.8",
    "electron-json-storage": "^4.1.6",
    "electron-remote": "^1.3.0",
    "env-cmd": "^9.0.3",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-standard": "^4.0.0",
    "ffmpeg": "^0.0.4",
    "file-url": "^3.0.0",
    "formatted": "^1.0.0",
    "fs-extra": "^8.0.1",
    "material-ui": "^0.20.2",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "npm-watch": "^0.6.0",
    "path": "^0.12.7",
    "prettier-eslint": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-datasheet": "^1.3.14",
    "react-dom": "^16.8.6",
    "react-file-drop": "^0.2.8",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "^4.11.0",
    "react-icons": "^3.7.0",
    "react-localization": "^1.0.13",
    "react-orbitjs": "^0.1.11",
    "react-player": "^1.11.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-select": "^3.0.4",
    "reactn": "^2.1.4",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "screenfull": "^4.2.0",
    "typescript": "^3.5.2",
