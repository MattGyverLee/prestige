{
  "name": "prestige",
  "version": "0.1.0",
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "mygo": "electron .",
    "web-dev": "env-cmd -f .env.web craco start --config craco.config.web.js",
    "web-prod": "set NODE_ENV=production&& env-cmd -f .env.web craco start --config craco.config.web.js",
    "electron-dev-mac": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev": "env-cmd -f .env.electron concurrently \"yarn start \" \"wait-on http://localhost:3000 && electron public/electron-dev.js\"",
    "electron-prod": "set NODE_ENV=production&& env-cmd -f .env.electron concurrently \"yarn start \" \"wait-on http://localhost:3000 && electron public/electron-dev.js\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "electron-builder": "^20.43.0",
    "env-cmd": "^9.0.3",
    "fs-extra": "^8.0.1",
    "moment": "^2.24.0",
    "npm-watch": "^0.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-datasheet": "^1.3.14",
    "react-file-drop": "^0.2.8",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.0",
    "react-localization": "^1.0.13",
    "react-orbitjs": "^0.1.11",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-select": "^3.0.4",
    "reactn": "^2.1.2",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.5.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.2.1",
    "@orbit/coordinator": "^0.15.23",
    "@orbit/core": "^0.15.23",
    "@orbit/data": "^0.15.23",
    "@orbit/indexeddb": "^0.15.25",
    "@orbit/jsonapi": "^0.15.25",
    "@orbit/store": "^0.15.25",
    "@testing-library/react": "^8.0.1",
    "@types/auth0-js": "^9.10.1",
    "@types/classnames": "^2.2.7",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.19",
    "concurrently": "^4.1.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-debug": "^2.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-react-devtools": "^0.5.3",
    "jest-dom": "^3.2.2",
    "wait-on": "^3.2.0"
  },
  "author": {
    "name": "Matthew Lee",
    "email": "langtech_cameroon@sil.org",
    "url": "https://LangTechCameroon.info"
  },
  "build": {
    "appId": "info.LangTechCameroon.prestige",
    "productName": "Prestige",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
