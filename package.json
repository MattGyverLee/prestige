    "eslint-config-eslint": "^5.0.1",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-promise": "^2.0.2",
    "eslint-config-react-app": "^4.0.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-jest": "^22.7.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "jest-dom": "^3.5.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "author": {
    "name": "Matthew Lee",
    "email": "langtech_cameroon@sil.org",
    "url": "https://LangTechCameroon.info"
  },
  "build": {
    "appId": "info.LangTechCameroon.prestige",
    "productName": "Prestige",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "resolutions": {
      "browserslist": "4.6.2",
      "caniuse-lite": "1.0.30000974"
    }
  }
