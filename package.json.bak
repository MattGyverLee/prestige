{
  "name": "prestige",
  "version": "0.1.0",
  "description": "consider yourself described",
  "homepage": "./",
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "DON'T craco eject",
    "dist": "electron-builder",
    "dist-d": "set DEBUG=electron-builder && electron-builder",
    "web-dev": "env-cmd -f .env.web craco start --config craco.config.web.js",
    "web-prod": "set NODE_ENV=production && env-cmd -f .env.web craco start --config craco.config.web.js",
    "electron-dev-mac": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev": "env-cmd -f .env.electron concurrently \"yarn start \" \"wait-on http://localhost:3000 && electron public/electron-dev.js\"",
    "electron-debug": "env-cmd -f .env.electron craco start",
    "electron-prod": "set NODE_ENV=production && env-cmd -f .env.electron concurrently \"yarn start \" \"wait-on http://localhost:3000 && electron public/electron.js\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-pack": "env-cmd -f .env.electron craco build -w --config craco.config.electron.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier --write \"src/**/*.js\"",
    "prelint": "eslint src --ext .tsx --ext .js",
    "precommit": "lint-staged",
    "lint": "eslint src --ext .tsx --ext .js",
    "lint-fix": "eslint src --fix --ext .tsx --ext .js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ]
  },
  "dependencies": {
    "@types/webpack-env": "^1.14.0",
    "chokidar": "^3.0.2",
    "electron-is-dev": "^1.1.0",
    "env-cmd": "^9.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "ffmpeg": "^0.0.4",
    "ffprobe": "^1.1.0",
    "file-url": "3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.0.1",
    "mime": "^2.4.4",
    "node-sass": "^5.0.0",
    "notistack": "^0.8.9",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-player": "^1.11.1",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.0.1",
    "react-sizeme": "^2.6.7",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.3",
    "sass": "^1.32.7",
    "screenfull": "^4.2.0",
    "wavesurfer.js": "^4.5.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.12.13",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/helper-define-map": "^7.12.13",
    "@babel/helper-regex": "^7.10.5",
    "@craco/craco": "^5.2.3",
    "@devexpress/dx-core": "^1.11.1",
    "@devexpress/dx-grid-core": "^1.11.1",
    "@devexpress/dx-react-core": "^1.11.1",
    "@devexpress/dx-react-grid": "^1.11.1",
    "@devexpress/dx-react-grid-material-ui": "^1.11.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^8.0.5",
    "@types/auth0-js": "^9.10.2",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-select": "^2.0.19",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.1",
    "@types/wavesurfer.js": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "browserslist": "^4.6.2",
    "concurrently": "^4.1.0",
    "depcheck": "^0.8.2",
    "electron": "^5.0.4",
    "electron-builder": "^20.10.4",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "ffmpeg-static": "^4.2.7",
    "ffprobe-static": "^3.0.0",
    "fibers": "^5.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^9.1.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-fontawesome": "^1.6.1",
    "react-refresh": "^0.9.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0",
    "yarn": "^1.17.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint src --ext .tsx --ext .js",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "author": {
    "name": "Matthew Lee",
    "email": "langtech_cameroon@sil.org",
    "url": "https://LangTechCameroon.info"
  },
  "build": {
    "appId": "info.LangTechCameroon.prestige",
    "productName": "Prestige",
    "copyright": "Copyright Â© 2019 ${author}",
    "extraFiles": [
      {
        "from": "bin/${os}/${arch}",
        "to": "resources/bin/${os}/${arch}",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "assets/icons/iceberg.ico"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
